{
  "compilerOptions": {
    "target": "es2016",
     "jsx": "preserve",
     "module": "ESNext",
     "paths": {},
     "resolveJsonModule": true,
     "allowJs": true,
     "sourceMap": true,
     "isolatedModules": true,
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "strict": true,
     "noImplicitAny": true,
     "strictNullChecks": true,
     "alwaysStrict": true,
     "skipLibCheck": true,
   "moduleResolution": "Node"
  }
}
